    "html-minifier": "^4.0.0",
    "husky": "^4.3.8",
    "jasmine": "^3.10.0",
    "jest": "^27.3.1",
    "json-6": "^1.1.4",
    "lit-node": "^0.1.1",
    "mocha": "^8.4.0",
